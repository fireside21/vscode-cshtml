{
    "name": "cshtml",
    "displayName": "HTML (C#)",
    "description": "HTML with embedded C#",
    "version": "0.1.2",
    "author": {
        "name": "Michael",
        "email": "michael@fireside21.com"
    },
    "publisher": "fireside21",
    "homepage": "https://github.com/fireside21/vscode-cshtml/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/fireside21/vscode-cshtml/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fireside21/vscode-cshtml.git"
    },
    "engines": {
        "vscode": "^1.26.0"
    },
    "icon": "images/icon-cshtml.png",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "cshtml",
            "aliases": [
                "HTML (C#)",
                "HTML with embedded C#",
                "ASP.NET",
                "aspx",
                "cshtml",
                "Master Page"
            ],
            "extensions": [
                ".aspx",
                ".ascx",
                ".cshtml",
                ".master"
            ],
            "firstLine": "^\\s*<%@",
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cshtml",
            "scopeName": "text.html.cshtml",
            "path": "./syntaxes/cshtml.tmLanguage.json"
        }],
        "snippets": [{
            "language": "cshtml",
            "path": "./snippets/cshtml.snippets.json"
        }]
    },
    "scripts": {
        "pack": "vsce package",
        "pub": "vsce publish"
    }
}
